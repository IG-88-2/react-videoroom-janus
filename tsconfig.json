{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es6",
    "module":"es2015",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "strict": false,
    "strictNullChecks": false,
    "sourceMap": true,
    "declaration": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "react-videoroom-janus.tsx"
  ]
}
